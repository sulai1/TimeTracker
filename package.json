{
  "name": "tracker",
  "displayName": "tracker",
  "description": "track work in vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Tracker",
      "properties": {
        "tracker.database.database": {
          "type": "string",
          "default": "postgres",
          "description": "The database to connect to"
        },
        "tracker.database.host": {
          "type": "string",
          "default": "localhost",
          "description": "The host to connect to"
        },
        "tracker.database.port": {
          "type": "number",
          "default": 5432,
          "description": "The port to connect to"
        },
        "tracker.database.user": {
          "type": "string",
          "default": "postgres",
          "description": "The user to connect as"
        },
        "tracker.database.password": {
          "type": "string",
          "default": "",
          "description": "The password to connect with"
        }
      }
    },
    "commands": [
      {
        "command": "tracker.connect",
        "title": "Tracker: Connect"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack && cpx \"migrations/*\" dist/migrations",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src",
    "test": "vscode-test",
    "mocha": "mocha --config src/unit/mocharc.js --watch"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@types/chai": "^5.0.1",
    "@types/pg": "^8.11.10",
    "cpx": "^1.5.0",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "chai": "^4.0.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "isomorphic-git": "^1.27.1",
    "pg": "^8.13.1",
    "simple-git": "^3.27.0",
    "tracer": "^1.3.0",
    "uuid": "^11.0.2",
    "zod": "^3.23.8"
  }
}